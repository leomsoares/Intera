
@{
    ViewBag.Title = "Projects";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="container" style="margin-top: 80px;">

    <div class="row">
        <input type="text" class="form-control input-search" placeholder="search...">
        <button class="form-control btn btn-default button-search" type="submit"><span class="glyphicon glyphicon-search"></span></button>
    </div>

    <div>
        <div class="nav-left">
            <h3 class="text-center">Professors</h3>
            <ul>
                <li>
                    <input type="checkbox" id="prof1"/>
                    <label for="prof1">Viana</label>
                </li>
                <li>
                    <input type="checkbox" id="prof2" />
                    <label for="prof2">Sergio</label>
                </li>
                <li>
                    <input type="checkbox" id="prof3" />
                    <label for="prof3">Edes</label>
                </li>
            </ul>

            <h3>Project</h3>
            <ul>
                <li>
                    <input type="checkbox" id="theme1" />
                    <label for="theme1">Scientific Research</label>
                </li>
                <li>
                    <input type="checkbox" id="theme2" />
                    <label for="theme2">TCC</label>
                </li>
            </ul>
        </div>

        <div class="div-projects">
            <div>
                <ul>
                    <li class="item-project">
                        <img src="data:image/svg+xml;base64,"
                             class="img-rounded">
                        <div class="info-seek">
                            <label class="label-seek">Project1</label>
                            <label class="label-seek">Description</label>
                            <label class="label-seek">Authors</label>
                        </div>
                        <div class="info-seek pull-right">
                            <a class="btn btn-default btn-seek" href="/project/seeproject"><span class="glyphicon glyphicon-eye-open"></span> View</a>
                            <button class="btn btn-default btn-seek"><span class="glyphicon glyphicon-download-alt"></span> Download</button>
                        </div>
                    </li>
                    <li  class="item-project">
                        <img src="data:image/svg+xml;base64,"
                             class="img-rounded">
                        <div class="info-seek" >
                            <label class="label-seek">Project2</label>
                            <label class="label-seek">Description</label>
                            <label class="label-seek">Authors</label>
                        </div>
                        <div class="info-seek pull-right">
                            <button class="btn btn-default btn-seek"><span class="glyphicon glyphicon-eye-open"></span> View</button>
                            <button class="btn btn-default btn-seek"><span class="glyphicon glyphicon-download-alt"></span> Download</button>
                        </div>
                    </li>
                    <li  class="item-project">
                        <img src="data:image/svg+xml;base64,"
                             class="img-rounded">
                        <div class="info-seek">
                            <label class="label-seek">Project3</label>
                            <label class="label-seek">Description</label>
                            <label class="label-seek">Authors</label>
                        </div>
                        <div class="info-seek pull-right">
                            <button class="btn btn-default btn-seek"><span class="glyphicon glyphicon-eye-open"></span> View</button>
                            <button class="btn btn-default btn-seek"><span class="glyphicon glyphicon-download-alt"></span> Download</button>
                        </div>
                    </li>
                </ul>
            </div>
        </div>
    </div>
</div>