@model IEnumerable<Intera.Entity.Projeto>
@{
    ViewBag.Title = "The Project";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@foreach (var t in Model)
{
<div style="margin-top:100px;">
    <div>
        <h2 class="" style="font-weight:bolder;">See Project</h2>
    </div>
    <div class="container" style="margin-top:10px;">
        <br />
        <div class="panel panel-default">
            <div class="panel-heading" style="font-weight: 900; font-size:larger;">Name Project</div>
            <div class="panel-body"> @t.NomeProjeto</div>


            <div class="panel-heading" style="font-weight: 900; font-size:larger;">Authors</div>
            @foreach (var U in ViewBag.Membros)
            {
                <div class="panel-body">@U.Nome</div>
            }
            <div class="panel-heading" style="font-weight: 900; font-size:larger;">Advisor</div>
            @foreach (var T in ViewBag.ProfessorNome)
            {
                <div class="panel-body">@T.Nome</div>
            }
            


            <div class="panel-heading" style="font-weight: 900; font-size:larger;">Summary</div>
            <div class="panel-body">
                @t.Descricao
            </div>
        </div>
        <br />


        <hr class="hrProject">

        <div class="row" style="height:50px;">

            <a class="btn btn-default btn-lg pull-right btnProject" href="#"><span class="glyphicon glyphicon-download"></span> Download</a>
            @*<a class="btn btn-default btn-lg pull-right btnProject" href="#"><span class="glyphicon glyphicon-arrow-left"></span> Back</a>*@
        </div>
    </div>
</div>

}