@model IEnumerable<Intera.Entity.Pessoa>

<table class="table table-bordered">
    <tr>
        <th>NAME</th>
        <th>E-MAIL</th>
        <th>#</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.Nome</td>
            <td>@item.Email</td>
            <td>
                <a href="/project/addalunodata/@item.IdPessoa" class="btn btn-default btn-sm"><i class="fa fa-plus"></i></a>
            </td>
        </tr>
    }
</table>
